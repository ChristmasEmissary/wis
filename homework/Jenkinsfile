    stage('docker image build') {
        sh "echo "
        sh 'pwd'
        sh 'cd homework'
        sh 'pwd'
        sh 'ls -al'
        sh 'echo $BUILD_NUMBER'
    }
    stage('dev') {
        echo "tag docker image dev pass"
        sh "echo dev stage pass"

    }
    stage('test') {
        sh "echo test stage pass"
    }
    stage('prod') {
        sh "echo deploy to prod"
    }


}